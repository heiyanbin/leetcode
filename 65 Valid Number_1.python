// 65
// Valid Number
// https://leetcode.com//problems/valid-number/
class Solution:
    # @param s, a string
    # @return a boolean
    def isNumber(self, s):
        i, n = 0, len(s)
        while i < n and s[i]==' ': i += 1
        if i < n and s[i] in '+-': i += 1
        isNum = False
        while i < n and s[i].isdigit():
            i += 1
            isNum = True
        if i < n and s[i] =='.':
            i += 1
            while i < n and s[i].isdigit():
                i += 1
                isNum = True
        if i < n and isNum and s[i] == 'e':
            i += 1
            isNum = False
            if i < n and s[i] in '+-': i += 1
            while i < n and s[i].isdigit():
                i += 1
                isNum = True
        while i < n and s[i] == ' ': i += 1
        return isNum and i == n
        